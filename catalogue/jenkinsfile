pipeline {
    agent {
        label "agent_1"
    }
    options {
        timeout(time:30, unit: 'MINUTES')
        disableConcurrentBuilds()
    }
    stages {
        stage('build') {
            steps {
                script {
                    sh """
                        echo "Hello Build"
                        sleep 10
                    """
                }
                
            }
        }
        stage('test') {
            steps {
                script {

                    sh """
                        echo "Hello pipeline testing"
                    """
                }
            }
        }
    }
}